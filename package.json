{
  "name": "rdf-tools",
  "version": "0.4.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Steffan Sluis <steffansluis@gmail.com>",
  "license": "MIT",
  "bin": {
    "rdf-tools": "dist/index.js"
  },
  "scripts": {
    "dist": "tsc && chmod +x dist/index.js",
    "clean": "rm -rf dist",
    "pretest": "npm run clean && tsc --inlineSourceMap",
    "test": "nyc ava --verbose",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "tsc --watch & nodemon; wait"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "@types/globby": "^6.1.0",
    "@types/n3": "^0.0.3",
    "@types/node": "^8.5.2",
    "@types/yargs": "^10.0.1",
    "globby": "^7.1.1",
    "json-format": "^1.0.1",
    "n3": "^0.11.2",
    "rdf": "^3.0.1",
    "semantic-toolkit": "^0.2.1",
    "typescript": "^2.6.2",
    "typescript-formatter": "^7.0.1",
    "yargs": "^10.1.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^1.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.1.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "description": "Tools for RDF",
  "repository": "git@github.com:knowledge-express/rdf-tools.git"
}
